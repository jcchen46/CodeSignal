int uniqueDigitProducts(vector<int> a) {
    set<int> s;
    for(int i = 0; i < a.size(); i++){
        int t = a[i]%10;
        a[i] /= 10;
        while(a[i]%10 != 0){
            t *= a[i]%10;
            a[i] /= 10;
        }
        if(s.find(t) == s.end())    s.insert(t);
    }
    return s.size();
}
